{
  "name": "SideEye",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "yarn pyinstaller && electron-webpack dev",
    "compile": "yarn pyinstaller && electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "pyinstaller": "rm -rf static/apidist && pyinstaller api/api.py --distpath static/apidist"
  },
  "dependencies": {
    "history": "^4.7.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router": "^4.3.1",
    "source-map-support": "^0.5.9"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0",
    "electron": "3.0.7",
    "electron-builder": "^20.29.0",
    "electron-webpack": "^2.3.1",
    "prettier": "^1.14.3",
    "webpack": "^4.23.1"
  },
  "resolutions": {
    "upath": "^1.0.5"
  },
  "electronWebpack": {
    "renderer": {
      "webpackConfig": "webpack.renderer.config.js"
    }
  },
  "build": {
    "extraResources": [
      "static/apidist/api/**"
    ]
  }
}
