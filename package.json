{
  "name": "SideEye",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "yarn pyinstaller && electron-webpack dev",
    "compile": "yarn pyinstaller && electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "pyinstaller": "rm -rf static/apidist && pyinstaller --hidden-import flask --hidden-import sideeye api/api.py --distpath static/apidist"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.7.0",
    "@blueprintjs/select": "^3.2.1",
    "csv-parse": "^3.1.3",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.11",
    "promise-file-reader": "^1.0.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router": "^4.3.1",
    "source-map-support": "^0.5.9",
    "styled-components": "^4.0.3",
    "upath": "^1.0.5"
  },
  "devDependencies": {
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "electron": "^5.0.3",
    "electron-builder": "^20.43.0",
    "electron-webpack": "^2.6.2",
    "flow-bin": "^0.85.0",
    "prettier": "^1.14.3",
    "webpack": "^4.23.1"
  },
  "resolutions": {
    "upath": "^1.0.5"
  },
  "electronWebpack": {
    "renderer": {
      "webpackConfig": "webpack.renderer.config.js"
    }
  },
  "build": {
    "extraResources": [
      "static/apidist/api/**"
    ]
  }
}
