{
  "name": "SideEye",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "yarn pyinstaller && electron-webpack dev",
    "compile": "electron-webpack && yarn pyinstaller",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "pyinstaller": "rm -rf static/apidist && pyinstaller --add-data 'dist/renderer:renderer' --hidden-import flask --hidden-import sideeye api/api.py --distpath static/apidist"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.7.0",
    "@blueprintjs/select": "^3.2.1",
    "@types/axios": "^0.14.0",
    "@types/request": "^2.48.3",
    "axios": "^0.19.0",
    "csv-parse": "^3.1.3",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.11",
    "promise-file-reader": "^1.0.2",
    "react": "^16.6.0",
    "react-dom": "^16.9.0",
    "request": "^2.88.0",
    "source-map-support": "^0.5.9",
    "styled-components": "^4.0.3",
    "upath": "^1.0.5"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/enzyme": "^3.9.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.14",
    "@types/lodash": "^4.14.134",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/styled-components": "^4.1.16",
    "electron": "^5.0.3",
    "electron-builder": "^20.43.0",
    "electron-webpack": "^2.6.2",
    "electron-webpack-ts": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^24.8.0",
    "prettier": "^1.14.3",
    "style-loader": "^0.23.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.5.1",
    "webpack": "^4.23.1"
  },
  "resolutions": {
    "upath": "^1.0.5"
  },
  "electronWebpack": {
    "renderer": {
      "webpackConfig": "webpack.renderer.config.js"
    }
  },
  "build": {
    "extraResources": [
      "static/apidist/api/**"
    ]
  }
}
